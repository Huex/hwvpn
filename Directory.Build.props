<?xml version="1.0" encoding="utf-8"?>
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <Choose>
    <!-- Autogenerated project for wpf -->
    <When Condition="$(MSBuildProjectName.EndsWith('_wpftmp'))">
      <PropertyGroup>
        <IntermediateOutputPath>$(MSBuildThisFileDirectory)~obj\$(Configuration)\wpftmp\$(Platform)</IntermediateOutputPath>
        <OutputPath>$(IntermediateOutputPath)bin</OutputPath>
      </PropertyGroup>
    </When>

    <!-- Libs & Apps -->
    <Otherwise>
      <PropertyGroup>
        <BaseIntermediateOutputPath>$(MSBuildThisFileDirectory)~obj\$(Configuration)\$(MSBuildProjectName)\$(Platform)</BaseIntermediateOutputPath>
        <IntermediateOutputPath>$(MSBuildThisFileDirectory)~obj\$(Configuration)\$(MSBuildProjectName)\$(Platform)</IntermediateOutputPath>
        <BaseOutputPath>$(MSBuildThisFileDirectory)~bin\$(Configuration)\$(MSBuildProjectName)\$(Platform)</BaseOutputPath>
        <OutputPath>$(MSBuildThisFileDirectory)~bin\$(Configuration)\$(MSBuildProjectName)\$(Platform)</OutputPath>
        <PackageOutputPath>~package\$(Configuration)\$(MSBuildProjectName)\</PackageOutputPath>
      </PropertyGroup>
    </Otherwise>
  </Choose>
</Project>
